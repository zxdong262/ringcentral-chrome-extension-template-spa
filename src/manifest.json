{
  "name": "RingCentral Embeddable Voice with browser extension",
  "description": "RingCentral Embeddable Voice with browser extension",
  "version": "0.0.1",
  "permissions": [
    "http://*/",
    "https://*/",
    "storage",
    "activeTab",
    "tabs",
    "background",
    "tabCapture",
    "system.display",
    "identity"
  ],
  "page_action": {
    "default_icon": {
      "16": "icons/rc16.png",
      "32": "icons/rc32.png",
      "48": "icons/rc48.png",
      "128": "icons/rc128.png"
    }
  },
  "icons": {
    "16": "icons/rc16.png",
    "32": "icons/rc32.png",
    "48": "icons/rc48.png",
    "128": "icons/rc128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.html5beta.com/*"
      ],
      "exclude_matches": [],
      "js": [
        "./content.js"
      ],
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' https://ringcentral.github.io/ringcentral-embeddable/index.html; object-src 'self'",
  "manifest_version": 2
}